package ngs.view{  import flash.display.MovieClip;  import com.greensock.TweenLite;  import com.greensock.TweenMax;  import com.greensock.easing.*;  import com.greensock.plugins.*;    public class CoalBubbleChart extends MovieClip  {    public var registered:Boolean = false;    public var instanceList:Array = [];    public var def_width:Number;    public var def_height:Number;    public var numInstances:Number = 114;        public function CoalBubbleChart()     {      TweenPlugin.activate([ScrollRectPlugin]);      preTransitionState();    }        public function preTransitionState():void    {      if (!instanceList.length)       {        register();      }            for(var i:int = 0; i < instanceList.length; i++)      {        instanceList[i].alpha = 0;        instanceList[i].y = instanceList[i].def_y + 15;      }    }        public function postTransitionState():void    {      this.visible = true;      for (var i:int = 0; i < instanceList.length; i++)      {        instanceList[i].alpha = 1;        instanceList[i].y = instanceList[i].def_y;      }          }        public function register():void    {      for (var i:int = 0; i < numInstances; i++)      {        instanceList.push(getChildByName("instance" + i));                Math.round(instanceList[i].x);        Math.round(instanceList[i].y);                instanceList[i].def_y = instanceList[i].y;      }    }        public function transitionIn():void    {      this.visible = true;      preTransitionState();      for (var i:int = 0; i < instanceList.length; i++)      {        TweenLite.to(instanceList[i], 0.4, { alpha: 1, y: instanceList[i].def_y , ease: Back.easeOut, delay: 0.004 * (i * 1.5) });      }    }        public function transitionOut():void    {      for (var i:int = 0; i < instanceList.length; i++)      {        TweenLite.to(instanceList[i], 0.5, { alpha: 0, y: 0, ease: Expo.easeOut, delay: 0.004 * i });      }    }  }}
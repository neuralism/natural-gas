package ngs.model{  import ngs.model.Session;  import flash.events.Event;    public class NgasConsum extends Session  {    private var _consumption:Number = 0;    public var name:String = "ngasConsum";    public static const VALUE:String = "value";        public function set consumption(val:Number):void    {      trace("set Consumption in ngas class");      _consumption = val;      dispatchEvent(new Event(NgasConsum.VALUE));    }        public function get consumption():Number    {      return _consumption;    }  }}
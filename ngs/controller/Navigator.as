package ngs.controller{  import flash.events.Event;  import flash.events.EventDispatcher;  import flash.events.MouseEvent;  import com.greensock.TweenLite;  import com.greensock.plugins.TweenPlugin;  import com.greensock.plugins.TintPlugin;  import com.greensock.plugins.RemoveTintPlugin;  import com.greensock.easing.*;  import net.phrologue.utils.MouseEvents;  import ngs.model.Session;  public class Navigator extends EventDispatcher  {    public var session:Session;        public function Navigator()    {      TweenPlugin.activate([TintPlugin, RemoveTintPlugin]);    }        public function section(target:Object):void    {      MouseEvents.typical(target, handler);            target.bg.alpha = 0;            target.lock = function():void      {        MouseEvents.lock(target);        TweenLite.to(target.bg, 0.5, { alpha: 1, ease: Expo.easeOut } );        TweenLite.to(target.txt, 0.5, { tint: 0xffffff, ease: Expo.easeOut } );      }            target.unlock = function ():void      {        MouseEvents.unlock(target);        TweenLite.to(target.bg, 0.5, { alpha: 0, ease: Expo.easeOut } );        TweenLite.to(target.txt, 0.5, { removeTint: true, ease: Expo.easeOut } );      }                  function handler(event:MouseEvent):void      {        switch (event.type)        {          case MouseEvent.CLICK:            session.lockNavigation();            session.destinationIndex = target.target;            break;          case MouseEvent.ROLL_OVER:            TweenLite.to(target, 0.25, { y: -3, ease: Back.easeOut } );            TweenLite.to(target.txt, 0.5, { tint: 0x333333, ease: Expo.easeOut } );            break;          }                    case MouseEvent.ROLL_OUT:          {            TweenLite.to(target, 0.25, { y: 0 , ease: Back.easeOut } );            TweenLite.to(target.txt, 0.5, { removeTint: true, ease: Expo.easeOut } );            break;          }        }      }    }        public function lever(target:Object):void    {          }  }}